{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "admin-lte@3.1.0",
      "destination": "wwwroot/lib/admin-lte/"
    },
    {
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.bundle.min.js.map",
        "css/bootstrap.min.css",
        "css/bootstrap.min.css.map"
      ]
    },
    {
      "library": "air-datepicker@2.2.3",
      "destination": "wwwroot/lib/air-datepicker/",
      "files": [
        "js/datepicker.min.js",
        "css/datepicker.min.css",
        "css/datepicker.css"
      ]
    },
    {
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables/"
    },
    {
      "provider": "jsdelivr",
      "library": "animate.css@4.1.1",
      "destination": "wwwroot/lib/animate.css/",
      "files": [
        "animate.min.css",
        "animate.css"
      ]
    },
    {
      "library": "datatables.net-bs5@1.11.4",
      "destination": "wwwroot/lib/datatables.net-bs5/"
    },
    {
      "library": "axios@0.26.0",
      "destination": "wwwroot/lib/axios/"
    }
  ]
}